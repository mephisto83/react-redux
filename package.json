{
  "name": "react-reduc-test",
  "version": "0.0.1",
  "description": "A test of React + Redux workflow",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "test": "mocha  --timeout 15000  --watch --compilers js:babel-core/register test/**/*.spec.js 2>&1 | tee report",
    "test:watch": "npm test -- --watch",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/redux.js --config webpack.config.development.js",
    "build-dev": "webpack src/index.js dist/redux.js --config webpack.config.js",
    "build": "npm run build:lib",
    "version": "npm run build",
    "watch-html": "mkdirp build && nodemon --watch index.html --exec cp index.html build/index.html",
    "watch-js": "mkdirp build",
    "coverage-help": "istanbul help",
    "coverage": "gulp test",
    "serve": "node src/server.js",
    "start": "npm run watch-html & npm run watch-js & npm run serve"
  },
  "dependencies": {
    "es6-promise": "^3.0.2",
    "history": "^1.13.0",
    "isomorphic-fetch": "^2.2.0",
    "material-ui": "^0.13.1",
    "react": "^0.14.2",
    "react-addons-create-fragment": "^0.14.2",
    "react-addons-pure-render-mixin": "^0.14.2",
    "react-addons-transition-group": "^0.14.2",
    "react-addons-update": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc4",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.4",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "expect": "^1.12.2",
    "express": "^4.13.3",
    "gulp-babel": "^6.1.0",
    "gulp-babel-istanbul": "^0.10.1",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "merge-stream": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.0.4",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.3",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.4.1"
  }
}
